<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="SearchBuddyForm_BTF">
    <default-activity>filterUser</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>FilmStrip</managed-bean-name>
      <managed-bean-class>com.view.beans.filmStrip.UtilsBean</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>headerId</managed-bean-name>
      <managed-bean-class>java.math.BigDecimal</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__3">
      <managed-bean-name>mode</managed-bean-name>
      <managed-bean-class>java.lang.String</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__14">
      <managed-bean-name>SearchBuddyForm</managed-bean-name>
      <managed-bean-class>view.SearchBuddyForm</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="SearchBuddyForm">
      <page>/fragment/SearchBuddyForm.jsff</page>
    </view>
    <method-call id="HideFilmstrip">
      <method>#{backingBeanScope.FilmStrip._hideFilmStrip}</method>
      <outcome>
        <fixed-outcome>hide</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ShowFilmstrip">
      <method>#{backingBeanScope.FilmStrip._showFilmStrip}</method>
      <outcome>
        <fixed-outcome>show</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-call id="AddEditBuddyForm_BTF">
      <task-flow-reference>
        <document>/WEB-INF/taskflow/AddEditBuddyForm_BTF.xml</document>
        <id>AddEditBuddyForm_BTF</id>
      </task-flow-reference>
      <input-parameter id="__15">
        <name>mode</name>
        <value>#{pageFlowScope.mode}</value>
      </input-parameter>
      <input-parameter id="__16">
        <name>headerId</name>
        <value>#{pageFlowScope.headerId}</value>
      </input-parameter>
    </task-flow-call>
    <method-call id="Execute">
      <method>#{bindings.Execute.execute}</method>
      <outcome>
        <fixed-outcome>Execute</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="filterUser">
      <method>#{bindings.filterUser.execute}</method>
      <outcome>
        <fixed-outcome>filterUser</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__4">
      <from-activity-id>SearchBuddyForm</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>addEdit</from-outcome>
        <to-activity-id>HideFilmstrip</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>ShowFilmstrip</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>show</from-outcome>
        <to-activity-id>SearchBuddyForm</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>HideFilmstrip</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>hide</from-outcome>
        <to-activity-id>AddEditBuddyForm_BTF</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__10">
      <from-activity-id>AddEditBuddyForm_BTF</from-activity-id>
      <control-flow-case id="__11">
        <from-outcome>back</from-outcome>
        <to-activity-id>Execute</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__12">
      <from-activity-id>Execute</from-activity-id>
      <control-flow-case id="__13">
        <from-outcome>Execute</from-outcome>
        <to-activity-id>ShowFilmstrip</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__17">
      <from-activity-id>filterUser</from-activity-id>
      <control-flow-case id="__18">
        <from-outcome>filterUser</from-outcome>
        <to-activity-id>SearchBuddyForm</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
